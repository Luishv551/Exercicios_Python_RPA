    "\n",
    "posabrearq.hotkey('win', 'r')\n",
    "\n",
    "\n",
    "posabrearq.typewrite('notepad')\n",
    "\n",
    "posabrearq.press('enter')\n",
    "\n",
    "\n",
    "fecharjanela = posabrearq.getActiveWindow()\n",
    "fecharjanela.close()\n",
    "\n",
    "posabrearq.press ('tab')\n",
    "posabrearq.press('enter')"
